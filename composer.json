{
    "name": "letamanoir/proteus",
    "description": "Generate PHP classes from Protobuf files",
    "version": "0.0.1",
    "keywords": [
        "grpc"
    ],
    "homepage": "https://github.com/LeTamanoir/Iris",
    "license": "MIT",
    "authors": [
        {
            "name": "Martin Saldinger",
            "email": "martin.saldinger@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.4",
        "antlr/antlr4-php-runtime": "^0.9.1"
    },
    "require-dev": {
        "carthage-software/mago": "1.0.0-beta.28",
        "pestphp/pest": "^4.1",
        "symfony/var-dumper": "^7.3"
    },
    "autoload": {
        "psr-4": {
            "Proteus\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/mago format",
        "lint": "vendor/bin/mago analyze"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "carthage-software/mago": true,
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}